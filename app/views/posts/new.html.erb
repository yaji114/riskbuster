Posts#new
<%= form_with(model: @post, url: user_posts_path, local: true) do |f| %>
  <%= f.text_field :disaster %><br>
  <%= f.text_field :category %><br>
  <%= f.date_field :date %><br>
  <%= f.text_field :locate %><br>
  <%= f.text_area :content, placeholder: "Compose new post..." %><br>
  <%= f.submit "Post" %>
<% end %>
